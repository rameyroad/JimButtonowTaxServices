{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=${POSTGRES_HOST};Port=5432;Database=${POSTGRES_DB};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};SSLMode=Require;Trust Server Certificate=true;Pooling=true;MinPoolSize=1;MaxPoolSize=100",
    "AdminConnection": "Host=${POSTGRES_HOST};Port=5432;Database=${POSTGRES_DB};Username=${POSTGRES_ADMIN_USER};Password=${POSTGRES_ADMIN_PASSWORD};SSLMode=Require;Trust Server Certificate=true;Pooling=true;MinPoolSize=1;MaxPoolSize=10"
  },
  "Azure": {
    "PostgreSQL": {
      "ConnectionPooling": {
        "MinPoolSize": 1,
        "MaxPoolSize": 100,
        "ConnectionIdleLifetime": 300
      }
    }
  }
}
